CREATE TABLE users(
    user_id int(11) AUTO_INCREMET PRIMARY KEY not null,
    userName varchar(256) not null,
    userEmail varchar(256) not null,
    userUID varchar(256) not null,
    userPwd varchar(256) not null
);

